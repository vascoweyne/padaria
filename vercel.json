{
    "version": 2,
    "name": "padaria-vercel",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "POST",
                "GET",
                "PATCH",
                "DELETE"
            ]
        }
    ]
    
}